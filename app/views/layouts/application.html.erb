<!DOCTYPE html>
<html>
  <head>
    <title>ChessPrep</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= yield :js %>
  </head>

  <body>
  	<%= flash[:error] %>
  	<%= flash[:success] %>
    <div class="container">
    <div class="nav">
    <%= link_to "Drill", drill_path %>
    <%= link_to "Edit Moves", edit_path %> 
    </div>
    <%= yield %>
    </div>
  </body>
</html>
